#!/bin/bash
bash <<< 'w=0;s=0;for((i=0;i<${#WATER};i++));do c=${WATER:i:1};case $c in w)v=0;;a)v=1;;t)v=2;;e)v=3;;r)v=4;;esac;w=$((w*5+v));done;for((i=0;i<${#STIR};i++));do c=${STIR:i:1};case $c in s)v=0;;t)v=1;;i)v=2;;r)v=3;;esac;s=$((s*4+v));done;n=$((w+s));r="";while [ $n -gt 0 ];do d=$((n%8));case $d in 0)r="b$r";;1)r="e$r";;2)r="s$r";;3)r="t$r";;4)r="c$r";;5)r="h$r";;6)r="o$r";;7)r="l$r";;esac;n=$((n/8));done;echo "$r"'
